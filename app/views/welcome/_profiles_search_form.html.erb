<div class="container-fluid">
  <%= content_tag "div", id: "profiles-search-form", class: "form", data: {frameworks: @frameworks} do %>
    <%= form_tag profiles_path, method: "get", remote: true do %>
      <%= label_tag "" %>
      <%= text_field_tag :search, params[:search], id: "profiles-search-field", autocomplete: true, placeholder: "Search by framework (i.e. 'Rails')" %>

      <%= submit_tag "Search", class: "btn btn-primary btn-sm", id: "search-button", name: "commit" %>
    <% end %>
  <% end %>
</div>
