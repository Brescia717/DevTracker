<div class="jumbotron page-header" id="logo">
  <h1>DevTracker</h1>
  <h3>Connecting companies to developers</h3>
</div>

<div class="welcome-content">
  <div class="profiles-search">
    <%= render 'profiles_search_form' %>
  </div>

  <div class="results">
    <%= render 'results' %>
  </div>

  <div class="welcome-background">
    <div class="row">
      <div class="map col-md-9 col-xs-6">
        <%= render 'map' %>
      </div>

      <div class="sidebar col-md-3 col-xs-6">
        <%= render 'sidebar' %>
      </div>
    </div>
  </div>
</div>
